                                <b x="0" y="0.02" z="0"/>
                            </link>
                        </joint>
                    </joint>
                </motor>
            </joint>
        </motor>
    </joint>
    <!-- middle finger -->
    <joint name="middleSpread" type="revolute" minPos="0" maxPos=".1" r="0.01">
        <u x="-1" y="0" z="0"/>
        <b x="0.0" y="0.075" z="0.0"/>
