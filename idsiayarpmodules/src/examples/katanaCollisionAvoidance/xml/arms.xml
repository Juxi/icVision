                            <u x="0" y="0" z="-1"/>
                            <b x="0.0" y="0.02" z="0.0"/>
                            <link name="fingerTip" r="0.005">
                                <u x="1" y="0" z="0"/>
                                <b x="0" y="0.02" z="0"/>
                            </link>
                        </joint>
                    </joint>
                </motor>
            </joint>
        </motor>
    </joint>
    <!-- ring finger --> 
    <joint name="ringSpread" type="revolute" minPos="0" maxPos=".2" r="0.01">
        <u x="-1" y="0" z="0"/>
        <b x="0.0" y="0.07" z="-0.015"/>
        <motor name="ringPinky">
            <joint name="firstKnuckle" type="revolute" minPos="0" maxPos="1.5" r="0.01">
                <u x="0" y="0" z="-1"/>
                <b x="0.0" y="0.0" z="0.0"/>
                <joint name="secondKnuckle" type="revolute" minPos="0" maxPos="1.5" r="0.005">
                    <u x="0" y="0" z="-1"/>
                    <b x="0.0" y="0.03" z="0.0"/>
                    <joint name="lastKnuckle" type="revolute" minPos="0" maxPos="1.5" r="0.005">
                        <u x="0" y="0" z="-1"/>
                        <b x="0.0" y="0.02" z="0.0"/>
                        <link name="fingerTip" r="0.005">
                            <u x="1" y="0" z="0"/>
                            <b x="0" y="0.02" z="0"/>
                        </link>
                    </joint>
                </joint>
            </joint>
        </motor>
    </joint>
    <!-- pinky finger -->
    <joint name="pinkySpread" type="revolute" minPos="0" maxPos=".3" r="0.01">
        <u x="-1" y="0" z="0"/>
        <b x="0.0" y="0.06" z="-0.03"/>
        <motor name="ringPinky">
            <joint name="firstKnuckle" type="revolute" minPos="0" maxPos="1.5" r="0.01">
                <u x="0" y="0" z="-1"/>
                <b x="0.0" y="0.0" z="0.0"/>
                <joint name="secondKnuckle" type="revolute" minPos="0" maxPos="1.5" r="0.005">
                <u x="0" y="0" z="-1"/>
                <b x="0.0" y="0.03" z="0.0"/>
                    <joint name="lastKnuckle" type="revolute" minPos="0" maxPos="1.5" r="0.005">
                        <u x="0" y="0" z="-1"/>
                        <b x="0.0" y="0.02" z="0.0"/>
                        <link name="fingerTip" r="0.005">
                            <u x="1" y="0" z="0"/>
                            <b x="0" y="0.02" z="0"/>
                        </link>
                    </joint>
                </joint>
            </joint>
        </motor>
    </joint>
</motor>

<!-- /// WRIST -->
        </joint>
        </motor>
    </joint>
    </motor>
</joint>
</motor>

<!-- /// ELBOW -->
</joint>
</motor>     

<!-- /// SHOULDER -->
        </joint>
        </motor>
    </joint>
    </motor>
</joint>
</motor> 


