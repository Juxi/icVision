                            </joint></motor>
                            </joint></motor>
                            </joint></motor>
                        </link>
                        <link x="-.5" z="1" radius="0" length=".09">
                            <motor name="eyeTilt"><joint name="rightEyeTilt" minPos="-35" maxPos="15" x="1" radius="0">
                            <motor name="eyePan"><joint name="rightEyePan" minPos="-50" maxPos="50" y="-1" radius="0">
                            <motor name="eyeVerg"><joint name="rightEyeVerg" minPos="0" maxPos="90" y="1" radius="0">
                                <link z="1" radius=".001" length=".05"></link>
                            </joint></motor>
                            </joint></motor>
                            </joint></motor>
                        </link>
                        <!-- /EYES -->
                    </link>
                    <!-- /HEAD -->
                        
                        
                </joint>
                </motor>
                </joint>
                </motor>
                    
            </link>
            </joint>
            </motor>
        
        </link>
        </link>
