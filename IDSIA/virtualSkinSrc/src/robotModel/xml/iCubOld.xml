                    <link y="-1" radius="0" length=".07">
                    <sphere py="-.07" radius=".09"/>
                    <!--<cylinder radius="0.01" height="0.02" py="-.07" pz=".07" vz="1" detail="12"/>-->
                                
                        <!-- EYES -->
                        <link x=".5" z="1" radius="0" length=".09">
                            <motor name="eyeTilt" minPos="-35" maxPos="15"><joint name="leftEyeTilt" minPos="-35" maxPos="15" x="1" radius="0">
                            <motor name="eyePan" minPos="-50" maxPos="50"><joint name="leftEyePan" minPos="-50" maxPos="50" y="-1" radius="0">
