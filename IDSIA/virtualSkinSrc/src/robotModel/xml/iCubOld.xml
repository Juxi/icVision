            <motor name="NeckPitch" minPos="-40" maxPos="30">
            <joint name="NeckPitch" minPos="-40" maxPos="30" x="1" radius=".025" length=".05">
            <link y="-1" radius="0" length=".05"><!-- first neck joint to second neck joint -->
            
