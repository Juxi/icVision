                <joint name="NeckRoll" minPos="-70" maxPos="60" z="-1" radius=".025" length=".05">
                <motor name="NeckYaw" minPos="-55" maxPos="55">
                <joint name="NeckYaw" minPos="-55" maxPos="55" y="1" radius="0">
                        
